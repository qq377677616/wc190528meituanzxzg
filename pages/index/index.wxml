        <!-- </view> -->
    </view>
    <!-- 线 -->
    <view class="tab_list2">
        <view class="{{currtab==1? 'active':''}}"></view>
        <view class="{{currtab==2? 'active':''}}"></view>
        <view class="{{currtab==3? 'active':''}}"></view>
    </view>
