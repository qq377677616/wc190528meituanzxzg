<!--index.wxml-->
<view class="container">
    <view class="hander" bindtap="linkbrowse">
        <image mode="widthFix" src="http://game.flyh5.cn/resources/game/wechat/file/mtimg/hander_ipt.png" class="fenlei"></image>     
    </view>
    <view class="zhanwei"></view>
    <view class="imge_banal">
         <image mode="widthFix" src='http://game.flyh5.cn/resources/game/wechat/file/mtimg/page1_28.png' class="banal_box" bindtap="outpage"></image>
    </view>
    <!-- 三个选项 -->
    <view class="tab_list">
        <!-- <view> -->
            <image mode="widthFix" src="http://game.flyh5.cn/resources/game/wechat/file/mtimg/page1_24.png" bindtap="tabcheck" data-tab='1'></image>
        <!-- </view> -->
        <!-- <view> -->
            <image mode="widthFix" src="http://game.flyh5.cn/resources/game/wechat/file/mtimg/page1_23.png" bindtap="tabcheck" data-tab='2'></image>
