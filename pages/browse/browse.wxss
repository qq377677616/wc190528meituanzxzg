.hander{
     width: 100%;
     height: 50px;
     background-color: white;
     position: fixed;
     left: 0;
     top: 0;
     display: flex;
     justify-content: space-between;
     padding: 10px 15px;
     box-sizing: border-box;
}
.iptbox{
     height: 30px;
     width: 80%;
     border-radius: 5px;
     box-shadow: 0px 0px 5px  #2eb3a4; 
}
.cencle{
    width:15%;
    text-align: center;
    height: 30px;
    line-height: 30px;
    color: #2eb3a4; 
    display: block;
    font-size: 14px;
    border-radius: 5px;
    box-shadow: 0px 0px 5px  #2eb3a4;
}
.searchicon{
    width: 20px;
    display: block;
    float: left;
    margin-top: 5px;
    margin-left: 8px;
}
.searchipt{
    display: inline-block;
    text-indent: 5px;
    margin-top: 3px;
    height: 20px;
    font-weight: 0;
    float: left;
    /* border: 1px solid #333; */
    color: #666;
    width: 75%; 
    /* background-color: red; */
}
.delicon{
    width: 25px;
    margin-top:3px; 
}
.psearch{
    font-size: 14px;
    line-height: 20px;
    /* margin-top: -3px; */
}
.setwidth{
      width: 100%;
      display: none;
      /* background-color: rebeccapurple; */
}
.imgtxt_list{
      width: 90%;
      margin: 0 auto;
      display: flex;
      padding-top: 10px;
      border-bottom: 1px solid #2eb3a4;
      padding-bottom: 15px;
}
.evencolor{
      border-bottom:1px solid #eb6438; 
}
.imgtxt_list .imgbox{
      width: 90px;
      height: 60px;
      display: inline-block;
      background-color: red;
      border-radius:5px;   
}
.imgtxt_list .txtbox{
      flex: 1;  
      padding-left: 16px;
      position: relative;
}
.txtbox .txt_title,.txtbox .txt_ct{
       font-size: 15px;
       font-weight: 600;
       display: inline-block;
       padding-right: 8px;
       border-right: 1px solid #000; 
}
.txtbox .txt_ct{
       font-weight: 100;
       padding-right: 0;
       border: 0;
       color: #656766;
       padding-left: 8px; 
} 
.txtbox image{
       position: absolute;
       width: 30px;
       display: inline-block;
       right: 0px;
       top: 8px;
}
.txttime,.times{
       font-weight: 100;
       font-size: 13px;
       display: block;
       color: #656766;
       padding-top: 8px;
       float: left;
}
.times{
       float: right;
       padding-right: 5px;
}
.link_img{
       width: 196px;
       position: fixed;
       left: 50%;
       bottom: 10px;
       transform: translateX(-50%);
}
.zhanwei{
       width: 100%;
       height: 50px;
       float: left;
}
.nofound,.nofoundtxt{
       width: 80px;
       position: fixed;
       left: 50%;
       top: 40%;
       transform: translate(-50%,-50%);
       display: none;
}
.nofoundtxt{
       top: 53%;
       width: 100px;  
}
.likepop{
       width: 100%;
       height: 50px;
       /* background-color: rebeccapurple; */
       float: left;
       line-height: 35px;
       padding: 0 10px;
       height: 35px;
       border-bottom:1px solid #dedede; 
}
